    "msft:region": "westeurope",
    "msft:group_id": "met-office-global-deterministic",
    "cube:variables": {
        "wind_speed": {
            "type": "data",
            "unit": "m/s",
            "dimensions": [
                "latitude",
                "longitude",
                "pressure_levels",
                "forecast:reference_datetime",
                "forecast:horizon"
            ],
            "description": "Wind speed on pressure levels is defined as a two-dimensional (horizontal) air velocity vector with no vertical component. The speed is the magnitude of velocity."
        },
        "air_temperature": {
            "type": "data",
            "unit": "K",
            "dimensions": [
                "latitude",
                "longitude",
                "pressure_levels",
                "forecast:reference_datetime",
                "forecast:horizon"
            ],
            "description": "Air temperature on pressure levels"
        },
        "relative_humidity": {
            "type": "data",
            "unit": "1",
            "dimensions": [
                "latitude",
                "longitude",
                "pressure_levels",
                "forecast:reference_datetime",
                "forecast:horizon"
            ],
            "description": "Fractional relative humidity (ratio of the partial pressure of water vapour to the equilibrium vapour pressure of water) on pressure levels"
        },
        "geopotential_height": {
            "type": "data",
            "unit": "m",
            "dimensions": [
                "latitude",
                "longitude",
                "pressure_levels",
                "forecast:reference_datetime",
                "forecast:horizon"
            ],
            "description": "Height above sea level of the pressure levels. Note: from January 2026, this parameter will be replaced by geopotential_height_on_pressure_levels"
        },
        "upward_air_velocity": {
            "type": "data",
            "unit": "m/s",
            "dimensions": [
                "latitude",
                "longitude",
                "pressure_levels",
                "forecast:reference_datetime",
                "forecast:horizon"
            ],
            "description": "Speed of the vertical component of the air motion at a pressure levels. Upwards is positive and downwards is negative."
        },
        "wind_from_direction": {
            "type": "data",
            "unit": "degrees",
            "dimensions": [
                "latitude",
                "longitude",
                "pressure_levels",
                "forecast:reference_datetime",
                "forecast:horizon"
            ],
            "description": "Wind direction on pressure levels is defined as a two-dimensional (horizontal) air velocity vector with no vertical component. In meteorological reports the direction of the wind vector is given as the direction from which it is blowing."
        },
        "wet_bulb_potential_temperature": {
            "type": "data",
            "unit": "K",
            "dimensions": [
                "latitude",
                "longitude",
                "pressure_levels",
                "forecast:reference_datetime",
                "forecast:horizon"
            ],
            "description": "Wet bulb potential temperature on pressure levels. Wet bulb potential temperature is defined as the temperature that a parcel of air would have if it were brought down to the standard pressure of 1000hPa following a saturated adiabatic lapse rate."
        }
    },

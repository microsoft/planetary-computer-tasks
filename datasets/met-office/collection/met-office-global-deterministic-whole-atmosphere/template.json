        },
        "cloud_amount_of_total_cloud": {
            "type": "application/netcdf",
            "roles": [
                "data"
            ],
            "title": "Cloud amount of total cloud",
            "forecast:variable": "cloud_area_fraction"
        },
        "CAPE_mixed_layer_lowest_500m": {
            "type": "application/netcdf",
            "roles": [
                "data"
            ],
            "title": "CAPE mixed layer lowest 500m",
            "forecast:variable": "atmosphere_convective_available_potential_energy"
        },
        "cloud_amount_of_medium_cloud": {
            "type": "application/netcdf",
            "roles": [
                "data"
            ],
            "title": "Cloud amount of medium cloud",
            "forecast:variable": "medium_type_cloud_area_fraction"
        },
        "cloud_amount_below_1000ft_ASL": {
            "type": "application/netcdf",
            "roles": [
                "data"
            ],
            "title": "Cloud amount below 1000ft ASL",
            "forecast:variable": "cloud_area_fraction_assuming_only_consider_surface_to_1000_feet_asl"
        },
        "CIN_most_unstable_below_500hPa": {
            "type": "application/netcdf",
            "roles": [
                "data"
            ],
            "title": "CIN most unstable below 500hPa",
            "forecast:variable": "atmosphere_convective_inhibition"
        },
        "CAPE_most_unstable_below_500hPa": {
            "type": "application/netcdf",

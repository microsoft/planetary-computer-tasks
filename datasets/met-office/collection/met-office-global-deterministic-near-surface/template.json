    "fog_fraction_at_screen_level": {
      "type": "application/netcdf",
      "title": "Fog fraction at screen level",
      "roles": [
        "data"
      ],
      "forecast:variable": "fog_area_fraction",
      "cube:dimensions": {
        "latitude": {
          "type": "spatial",
          "axis": "y",
          "extent": [
            -90,
            90
          ]
        },
        "longitude": {
          "type": "spatial",
          "axis": "x",
          "extent": [
            -180,
            180
          ]
        }
      },
      "cube:variables": {
        "fog_area_fraction": {
          "description": "Here fog means a visibility of 1000 m or lower. The reduction in visibility is caused water droplets or minute ice crystals forming close to the surface. This quantity represents the fraction of horizontal grid square occupied by fog. An alternative interpretation is that this represents the fractional probability of fog being present at any location in the grid square.",
          "dimensions": [
            "latitude",
            "longitude"
          ],
          "type": "data",
          "unit": "1"
        }
      }
    },
    "height_of_orography": {

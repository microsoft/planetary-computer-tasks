            "dimensions": [
                "latitude",
                "longitude",
                "height",
                "forecast:reference_datetime",
                "forecast:horizon"
            ],
            "description": "Wind direction on height levels is defined as a two-dimensional (horizontal) air velocity vector with no vertical component. In meteorological reports the direction of the wind vector is given as the direction from which it is blowing."
        },
        "cloud_volume_fraction_in_atmosphere_layer": {
            "type": "data",
            "unit": "1",
            "dimensions": [
                "latitude",
                "longitude",
                "height",
                "forecast:reference_datetime",
                "forecast:horizon"
            ],
            "description": "Fraction of horizontal grid square occupied by cloud in layers centred on height levels."
        }

            "dimensions": [
                "latitude",
                "longitude",
                "height",
                "forecast:reference_datetime",
                "forecast:horizon"
            ],
            "description": "Wind speed on height levels is defined as a two-dimensional (horizontal) air velocity vector with no vertical component. The speed is the magnitude of velocity."
        },
        "air_temperature": {
            "type": "data",
            "unit": "K",
            "dimensions": [
                "latitude",
                "longitude",
